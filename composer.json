{
    "name": "bnski/pipelike",
    "description": "A Laravel Pipeline Action Implementation",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Bnski\\Pipelike\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.3",
        "illuminate/support": "^10.0|^11.0",
        "laravel/serializable-closure": "^1.3"
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
